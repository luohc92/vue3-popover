{
  "name": "vue3-popover",
  "description": "a popover for vue3.x 一款vue3.x的弹出框",
  "keyword": "vue vue3.x popover",
  "license": "MIT",
  "author": "luohc92",
  "private": false,
  "version": "1.2.6",
  "type": "module",
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && vue-tsc -d --emitDeclarationOnly"
  },
  "main": "./dist/vue3-popover.umd.js",
  "module": "./dist/vue3-popover.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue3-popover.es.js",
      "require": "./dist/vue3-popover.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luohc92/vue3-popover.git"
  },
  "dependencies": {
    "vue": "^3.4.22"
  },
  "devDependencies": {
    "@types/node": "^18.7.23",
    "@types/vue": "^2.0.0",
    "@vitejs/plugin-vue": "^3.1.0",
    "sass": "^1.89.0",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vite-plugin-dts": "^3.7.3",
    "vue-tsc": "^0.40.4"
  }
}
